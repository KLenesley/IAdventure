<form method="post" action="{{ path('app_team_delete', {'id': team.id}) }}" class="d-inline" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ team.id) }}">
    <button class="{{ button_class|default('btn btn-outline-danger btn-sm') }}">Supprimer</button>
</form>
